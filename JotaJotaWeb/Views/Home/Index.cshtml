@{
    ViewData["Title"] = "Index";
}


<!--Carousel-->
<div id="myCarousel" class="carousel slide mb-4 " data-ride="carousel" data-interval="6000">
    <ol class="carousel-indicators">
        <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
        <li data-target="#myCarousel" data-slide-to="1"></li>
        <li data-target="#myCarousel" data-slide-to="2"></li>
        <li data-target="#myCarousel" data-slide-to="3"></li>
    </ol>
    <div class="carousel-inner" role="listbox">
        <!--Slider 1-->
        <div class="carousel-item active">
            <img src="~/images/Slider_0.svg" alt="Torres de transmision" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <h5>
                    Titulo
                </h5>
                <p>
                    Texto explicativo para el titulo
                    <br />
                    <a class="btn btn-primary" href="https://www.">
                        CTO
                    </a>
                </p>
            </div>
        </div>
        <!--Slider 2-->
        <div class="carousel-item">
            <img src="~/images/Slider_1.svg" alt="Construccion electrica" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <h5>
                    Titulo
                </h5>
                <p>
                    Texto explicativo para el titulo
                    <br />
                    <a class="btn btn-primary" href="https://www.">
                        CTO
                    </a>
                </p>
            </div>
        </div>
        <!--Slider 3-->
        <div class="carousel-item">
            <img src="~/images/Slider_2.svg" alt="Material Electrico" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <h5>
                    Titulo
                </h5>
                <p>
                    Texto explicativo para el titulo
                    <br />
                    <a class="btn btn-primary" href="https://www.">
                        CTO
                    </a>
                </p>
            </div>
        </div>
        <!--Slider 4-->
        <div class="carousel-item">
            <img src="~/images/Slider_3.svg" alt="Electrocomercial" class="img-responsive" />
            <div class="carousel-caption" role="option">
                <h5>
                    Titulo
                </h5>
                <p>
                    Texto explicativo para el titulo
                    <br />
                    <a class="btn btn-primary" href="https://www.">
                        CTO
                    </a>
                </p>
            </div>
        </div>
    </div>
    <a class="carousel-control-prev" href="#myCarousel" role="button" data-slide="prev">
        <span class="glyphicon glyphicon-chevron-left" aria-hidden="true"></span>
        <span class="sr-only">Previous</span>
    </a>
    <a class="carousel-control-next" href="#myCarousel" role="button" data-slide="next">
        <span class="glyphicon glyphicon-chevron-right" aria-hidden="true"></span>
        <span class="sr-only">Next</span>
    </a>
</div>
<!--End Carousel-->
<!--Conocenos-->

<div class="card bg-dark mb-4">
    <img class="card-img" src="~/images/Banner_0.png" alt="JJ Electrocomercial">
    <div class="card-img-overlay">
        <h1 class="card-title">J &amp; J Electrocomercial</h1>
        <hr />
        <p class="card-text">Somos una empresa con una simple mision. Hacer de tu obra un gran proyecto.</p>
        <p class="card-text">Proveemos material y equipo electrico asi como servicios de construccion y tramites ante <a class="lead" href="https://cfe.mx">CFE</a>.</p>
        <p class="card-footer">
            <a class="btn btn-primary btn-dark btn-lg" asp-controller="Home" asp-action="About" role="button">Conocenos</a>
        </p>
    </div>
</div>

<!--End Conocenos-->
<!--Tabs-->
<div class="jumbotron mi-jumbotron">
    <nav>
        <div class="nav nav-tabs" id="nav-tab" role="tablist">
            <a class="nav-item nav-link active" id="nav-home-tab" data-toggle="tab" href="#nav-home" role="tab" aria-controls="nav-home" aria-selected="true">Productos</a>
            <a class="nav-item nav-link" id="nav-profile-tab" data-toggle="tab" href="#nav-profile" role="tab" aria-controls="nav-profile" aria-selected="false">Servicios</a>
            <a class="nav-item nav-link" id="nav-contact-tab" data-toggle="tab" href="#nav-contact" role="tab" aria-controls="nav-contact" aria-selected="false">Proyectos</a>
        </div>
    </nav>
    <div class="tab-content" id="nav-tabContent">
        <!--Tab 1 Content-->
        <div class="tab-pane fade show active" id="nav-home" role="tabpanel" aria-labelledby="nav-home-tab">         
            @await Html.PartialAsync("/Views/PartialViews/ProductPartial.cshtml")
            <p class="lead">
                <a class="btn btn-primary btn-lg btn-block" asp-controller="Product" asp-action="Index" role="button">See all</a>
            </p>
        </div>
        <!--Tab 2 Content-->
        <div class="tab-pane fade" id="nav-profile" role="tabpanel" aria-labelledby="nav-profile-tab">
            @await Html.PartialAsync("/Views/PartialViews/ServicePartial.cshtml")
            <p class="lead">
                <a class="btn btn-primary btn-lg btn-block" asp-controller="Service" asp-action="Index" role="button">See all</a>
            </p>
        </div>
        <!--Tab 3 Content-->
        <div class="tab-pane fade" id="nav-contact" role="tabpanel" aria-labelledby="nav-contact-tab">
            <!--Card 1-->
            <div class="card d-inline-block mb-3 mr-3" style="width: 18rem;">
                <img class="card-img-top" src="~/images/286x180.svg" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">Proyecto 1</h5>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                    <a href="#" class="btn btn-primary">Go somewhere</a>
                </div>
            </div>
            <!--Card 2-->
            <div class="card d-inline-block mb-3 mr-3" style="width: 18rem;">
                <img class="card-img-top" src="~/images/286x180.svg" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">Proyecto 2</h5>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                    <a href="#" class="btn btn-primary">Go somewhere</a>
                </div>
            </div>
            <!--Card 3-->
            <div class="card d-inline-block mb-3 mr-3" style="width: 18rem;">
                <img class="card-img-top" src="~/images/286x180.svg" alt="Card image cap">
                <div class="card-body">
                    <h5 class="card-title">Proyecto 3</h5>
                    <p class="card-text">Some quick example text to build on the card title and make up the bulk of the card's content.</p>
                    <a href="#" class="btn btn-primary">Go somewhere</a>
